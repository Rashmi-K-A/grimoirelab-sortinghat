#!/Users/rka/Personal/GSoC/chaoss/grimoirelab-sortinghat/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'sortinghat==0.8.0.dev0','console_scripts','sortinghat'
__requires__ = 'sortinghat==0.8.0.dev0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('sortinghat==0.8.0.dev0', 'console_scripts', 'sortinghat')()
    )
